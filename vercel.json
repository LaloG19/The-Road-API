{
    "version": 2,
    "builds": [
      {
        "src": "src/index.ts",
        "use": "@vercel/node",
        "config": {
          "includeFiles": [
            "src/**",
            ".env"
          ]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/src/index.ts"
      }
    ],
    "env": {
      "PORT": "3000",
      "DB_URI": "@db_uri"
    }
  }
  